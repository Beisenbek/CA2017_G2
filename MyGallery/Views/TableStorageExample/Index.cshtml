@model IEnumerable<MyGallery.Models.BaseImage>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table>
    <tr>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.PartitionKey
        </td>
        <td>
            @item.RowKey
        </td>
        <td>
            @item.Name
        </td>
        <td>
            <img src=@item.URLcore>
        </td>
        <td>
            <img src=@item.URLcrop>
        </td>
    </tr>
}

</table>
